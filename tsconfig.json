{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@*": [
        "src/*"
      ],
      "@/*": [
        "src/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/lib/utils/*": [
        "./src/lib/utils/*"
      ]
    },
    "jsx": "preserve",
    "allowJs": true
  }
}